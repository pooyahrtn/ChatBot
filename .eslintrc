{
    "rules": {
      "no-console": 0,
      "no-underscore-dangle": 0,
      "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
      "no-use-before-define": ["error", { "variables": false }],
      "no-multi-str": 0,
      "indent":"off",
      "no-trailing-spaces":"off"
    },
    "env": {
      "node": true,
      "mocha": true
    },
    "parserOptions": {
      "ecmaVersion": 8
    },
    "extends": [
      "airbnb-base"
    ]
  }